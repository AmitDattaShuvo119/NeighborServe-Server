{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./chatApp.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "./chatApp.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}
